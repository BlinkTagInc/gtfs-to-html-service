{
  "name": "gtfs-to-html-service",
  "version": "1.0.2",
  "private": false,
  "description": "A service for building HTML timetables from GTFS using gtfs-to-html.",
  "keywords": [
    "transit",
    "gtfs",
    "transportation"
  ],
  "repository": "https://github.com/BlinkTagInc/gtfs-to-html-service.git",
  "license": "MIT",
  "author": "Brendan Nee <brendan@blinktag.com>",
  "scripts": {
    "build": "next build",
    "dev": "node -r dotenv/config server.mjs",
    "start": "NODE_ENV=production node -r dotenv/config server.mjs",
    "test": "xo"
  },
  "xo": {
    "esnext": true,
    "space": true
  },
  "dependencies": {
    "@auth0/s3": "^1.0.0",
    "@hapi/hapi": "^21.3.0",
    "@hapi/inert": "^7.1.0",
    "acorn": "^8.8.2",
    "dotenv": "16.0.3",
    "gtfs-to-html": "^2.5.1",
    "hapi-require-https": "^6.0.0",
    "lodash-es": "^4.17.21",
    "next": "^12.2.5",
    "node-fetch": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "tmp-promise": "^3.0.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "xo": "0.53.1"
  },
  "engines": {
    "node": "18.x"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md"
      }
    }
  }
}
